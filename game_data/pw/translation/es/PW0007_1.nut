translation.add(244, "I'm going. I just came to bring you your lunch\nboxes.", "Suzushiro");
translation.add(245, "She must have come from the classroom. She thrusts\nthe package she's carrying at me, then she turns\naround and walks off to the exit.", "");
translation.add(246, "Ah, Suzushiro!", "Chris");
translation.add(247, "Just a minute!", "Arata");
translation.add(248, "I reach out to her shoulder to stop her.", "");
translation.add(249, "Ahh..........", "");
translation.add(250, "Damn. I shouldn't touch her. But it's already too\nlate.", "");
translation.add(251, "Before my hand reaches her, something hard knocks\nit away.", "");
translation.add(252, "Please don't touch me!!!", "Suzushiro");
