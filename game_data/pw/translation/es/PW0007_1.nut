translation.add(235, "Please don't worry. I didn't hear anything.", "Suzushiro");
translation.add(236, "When I got here, the two of you were just staring\nat each other.", "Suzushiro");
translation.add(237, "I, I see...", "Chris");
translation.add(238, "Whew......", "Arata");
