translation.add(210, "I felt so embarrassed I thought I was going to\ndie.", "Chris");
translation.add(211, "W, well, I felt like I was going to die, too...\nYeah.", "Arata");
translation.add(212, "I, I see... Hmm...", "Chris");
translation.add(213, "Yeah.........", "Arata");
translation.add(214, "Mmmh..............", "Chris");
translation.add(215, "..................", "Arata");
translation.add(216, "..................", "Chris");
translation.add(217, "We've run out of things to say. My head drops.", "");
translation.add(218, "..................", "Arata");
translation.add(219, "..................", "Chris");
translation.add(220, "But my eyes don't move away from her face. It's a\nbit like that situation yesterday.", "");
translation.add(221, "Damn, my cheeks are getting hot.", "");
translation.add(222, "How did we end up talking about that?", "");
translation.add(223, "I've got to say something...... Anything......", "");
translation.add(224, "..........What are you staring at each other for?", "Suzushiro");
translation.add(225, "Whaaaaaaaaaaa!!", "Arata & Chris");
