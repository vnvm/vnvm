translation.add(327, "Don't...... Don't take any more of that which I\nhold dear!!!", "Chris");
translation.add(328, "You...... You're taking away the prince that lives\ninside of me......", "Chris");
translation.add(329, "When you talk to me...... When I'm near you......\nThe prince gradually fades away.", "Chris");
translation.add(330, "It's supported me all these years, and you're\nkicking it away from under me!!!", "Chris");
translation.add(331, "Don't break anything else! Stop taking my prince\naway from me!!!", "Chris");
translation.add(332, "..................", "Arata");
translation.add(333, "No.", "Arata");
translation.add(334, ".........!!", "Chris");
translation.add(335, "I'm not gonna stop!", "Arata");
translation.add(336, "Why!!! Why, Arata!!!!", "Chris");
translation.add(337, "I want to be with you! I don't want to leave you,\never!!", "Arata");
translation.add(338, "......Stop it........ If you say it......... Then\nI......... I will......", "Chris");
translation.add(339, "It's okay!!", "Arata");
translation.add(340, "Don't say it!!", "Chris");
translation.add(341, "Listen!!", "Arata");
translation.add(342, "!?", "Chris");
translation.add(343, "......I'll give myself to you. You can have me.", "Arata");
translation.add(344, "If you lost something precious because of me, then\nyou can have me in return.", "Arata");
translation.add(345, "I'll become yours. I'll stay with you forever.", "Arata");
translation.add(346, "...........Why........ How can you say such a\nthing........", "Chris");
translation.add(347, "How can you offer yourself to me! You don't even\nknow me!!", "Chris");
translation.add(348, "Because I like you. A lot.", "Arata");
translation.add(349, "..............!", "Chris");
translation.add(350, "It's because I like you that much, see.", "Arata");
translation.add(351, "What... Do you mean...... Do you even know what\nyou're saying......?", "Chris");
translation.add(352, "No, not really. I don't know how many ways there\nare to like someone.", "Arata");
translation.add(353, "But, I like you!", "Arata");
translation.add(354, "I want to be with you!", "Arata");
translation.add(355, "Forever... Forever!!!", "Arata");
translation.add(356, "...................You idiot......... You\nidiot..........", "Chris");
translation.add(357, "She's trembling. She's biting her lip.", "");
translation.add(358, "A drop of water rolls down her face.", "");
translation.add(359, "You don't...... Have to hold it back.", "Arata");
translation.add(360, "No matter if you shout, no matter what you say,\nnothing's going to go away.", "Arata");
translation.add(361, "Because I completely belong to you......", "Arata");
translation.add(362, "Uuu......... Uuuuuu............ Sob...........\nAhhhh...........", "Chris");
translation.add(363, "..............!!", "Chris");
translation.add(364, "I embrace her, as tightly as I can.", "");
translation.add(365, "So that nobody else can hear her shaking voice...", "");
