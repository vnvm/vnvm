translation.add(158, "That's okay.", "Shizuka");
translation.add(159, "Okay, then I'm off!", "Arata");
translation.add(160, "I run back to my school building, hugging the\nlunch box to my chest.", "");
translation.add(161, "Sigh.........", "Shizuka");
translation.add(162, "Huh.........?", "Shizuka");
